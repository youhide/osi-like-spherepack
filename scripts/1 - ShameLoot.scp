[FUNCTION SHAMELOOT]
// 0 - NUMERO DE ITEMS
// 1 - UID DE QUEM MATOU
// 2 - POTENCIA DO MONSTRO
REF1 <ARGV[1]>
FOR 1 <EVAL <ARGV[0]>>
 IF <R1,100> <= 95
  SERV.NEWITEM I_KATANA
  NEW.CONT <UID>
  NEW.NAME WEAK KATANA
 ELSE
  SERV.NEWITEM I_KATANA
  NEW.CONT <UID>
  NEW.NAME WEAK KATANA
 ENDIF
ENDFOR
  

[EOF]